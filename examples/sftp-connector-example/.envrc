export TF_VAR_sftp_server_url="sftp://example.com:22"
# Either use an existing secret
# export TF_VAR_existing_secret_arn="arn:aws:secretsmanager:region:account-id:secret:secret-name"
# Or create a new secret with these credentials
export TF_VAR_sftp_username="sftp-user"
export TF_VAR_sftp_password="example-password"
